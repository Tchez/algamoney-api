# Configurações do banco de dados
spring.datasource.url=jdbc:mysql://localhost:3306/algamoneydb?useSSL=false&serverTimezone=UTC&allowPublicKeyRetrieval=true
spring.datasource.username=root
spring.datasource.password=rootpassword

# Configurando para o Hibernate criar as tabelas automaticamente no banco de dados
spring.jpa.show-sql=true

# Adicionando falha na serialização de propriedades desconhecidas
spring.jackson.deserialization.fail-on-unknown-properties=true
